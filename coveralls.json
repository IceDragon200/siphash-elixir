{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "defexception",
    "defprotocol",
    "defstruct",
    "def.+(.+\\\\.+).+do",
    "^\\s+use\\s+"
  ],
  "custom_stop_words": [
    "case :erlang.load_nif",
    "Logger",
    "log_msg"
  ],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": false
  }
}
